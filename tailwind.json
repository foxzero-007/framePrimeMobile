{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"flex-1": {
		"style": {
			"flexGrow": 1,
			"flexShrink": 1,
			"flexBasis": "0%"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"rounded-sm": {
		"style": {
			"borderTopLeftRadius": 2,
			"borderTopRightRadius": 2,
			"borderBottomRightRadius": 2,
			"borderBottomLeftRadius": 2
		}
	},
	"border-2": {
		"style": {
			"borderTopWidth": 2,
			"borderRightWidth": 2,
			"borderBottomWidth": 2,
			"borderLeftWidth": 2
		}
	},
	"border-4": {
		"style": {
			"borderTopWidth": 4,
			"borderRightWidth": 4,
			"borderBottomWidth": 4,
			"borderLeftWidth": 4
		}
	},
	"border-gray-500": {
		"style": {
			"--tw-border-opacity": 1,
			"borderTopColor": "rgb(107 114 128 / var(--tw-border-opacity, 1))",
